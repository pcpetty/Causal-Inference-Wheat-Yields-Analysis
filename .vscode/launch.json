    "configurations": [
        {
            "type": "R-Debugger",
            "name": "Launch R-Workspace",
            "request": "launch",
            "debugMode": "workspace",
            "workingDirectory": ""
        },
        {
            "type": "R-Debugger",
            "name": "Attach to R process",
            "request": "attach",
            "splitOverwrittenOutput": true
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "R-Debugger",
            "name": "Debug R-File",
            "request": "launch",
            "debugMode": "file",
            "workingDirectory": "",
            "file": ""
        },

        {
            "type": "chrome",
            "request": "launch",
            "name": "Open Impact-of-Cover-Crops-on-Wheat-Crop-Yields.nb.html",
